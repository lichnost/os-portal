<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='dashboard'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.gwtplatform.mvp.MvpWithEntryPoint" />
	<inherits name="com.gwtplatform.dispatch.rpc.DispatchRpc" />

	<!--GWT Material -->
	<inherits name="gwt.material.design.GwtMaterialBasicWithJQueryDebug" />
	<inherits name="gwt.material.design.themes.GwtMaterialThemeBlue" />
	<inherits name="gwt.material.design.addins.GWTMaterialAddins" />

	<inherits name="com.objectsync.portal.core.model.CoreModel" />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

	<extend-configuration-property name="gin.ginjector.modules"
		value="com.objectsync.portal.dashboard.client.gin.ClientModule" />

	<set-configuration-property name="CssResource.enableGss"
		value="true" />
	<!-- <set-configuration-property name="gwtp.bootstrapper" value="com.mydomain.myproject.client.MyBootstrapper"/> -->
</module>
